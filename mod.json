{
	"geode": "3.9.0",
	"gd": {
		"win": "2.206",
		"android": "2.206",
		"mac": "2.206",
		"ios": "2.206"
	},
	"id": "ruhax.nelispscontroll",
	"name": "Nelis PS",
	"version": "v1.4.0",
	"developer": "RuHax Team",
	"description": "NelisPSControll",
	"early-load": true,
	"dependencies": [
		{
			"id": "geode.node-ids",
			"importance": "required",
			"version": ">=1.12.0"
		}
	],
	"resources": {
		"sprites": [
			"resources/sprites/*.png",
			"resources/sprites/**/*.png"
		],
		"spritesheets": {
			"Main": [
				"resources/spritesheets/main/**/*.png"
			]
		},
		"files": [
			"resources/files/config/*",
			"resources/files/fonts/*",
			"resources/files/sounds/*"
		]
	},
	"settings": {
		"nothing": {
			"name": "nothing",
			"description": "nothing nothing nothing",
			"type": "bool",
			"default": true
		},
		"MenuLoop_Drum": {
			"name": "MenuLoop_Drum",
			"type": "float",
			"default": 0,
			"min": 0,
			"max": 1,
			"control": {
				"slider-step": 0.1,
				"slider": true
			}
		},
		"MenuLoop_Vocal": {
			"name": "MenuLoop_Vocal",
			"type": "float",
			"default": 0,
			"min": 0,
			"max": 1,
			"control": {
				"slider-step": 0.1,
				"slider": true
			}
		}
	},
	"issues": {
		"info": "You can report issues about Nelis PS mod in <cy>the mod's repository</c>.",
		"url": "https://github.com/RuHax-Team/NelisPSControll"
	},
	"repository": "https://github.com/RuHax-Team/NelisPSControll",
	"links": {
		"repository": "https://github.com/RuHax-Team/NelisPSControll"
	},
	"tags": [ "online", "enhancement", "music", "interface" ]
}
